; Minimal Godot 4 template project for Fab.
; Prefer editing project settings via the Godot editor.
config_version=5

[application]

config/name="FabGameTemplate"
run/main_scene="res://scenes/Main.tscn"

[autoload]

FabDojoConfig="*res://scripts/FabDojoConfig.gd"
FabDojoClient="*res://scripts/FabDojoClient.gd"
FabController="*res://scripts/FabController.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/mode=2

[rendering]

; Web exports require the Compatibility renderer (WebGL); Forward+/Mobile rely on Vulkan.
renderer/rendering_method="gl_compatibility"
