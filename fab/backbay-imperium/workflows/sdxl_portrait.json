{
  "metadata": {
    "name": "SDXL Leader Portrait Generator",
    "version": "1.0.0",
    "description": "Generates leader portraits using SDXL with classical painting style"
  },
  "nodes": {
    "1": {
      "class_type": "CheckpointLoaderSimple",
      "inputs": {
        "ckpt_name": "sd_xl_base_1.0.safetensors"
      }
    },
    "2": {
      "class_type": "CLIPTextEncodeSDXL",
      "inputs": {
        "text_g": "{{prompt}}, {{style_base}}, masterpiece, best quality",
        "text_l": "{{prompt}}, classical oil painting, detailed face, museum quality artwork",
        "clip": ["1", 1],
        "width": "{{width}}",
        "height": "{{height}}",
        "crop_w": 0,
        "crop_h": 0,
        "target_width": "{{width}}",
        "target_height": "{{height}}"
      }
    },
    "3": {
      "class_type": "CLIPTextEncodeSDXL",
      "inputs": {
        "text_g": "{{negative_prompt}}, deformed, bad anatomy, bad hands, missing fingers",
        "text_l": "cartoon, anime, 3d render, photograph, modern, low quality, blurry, watermark",
        "clip": ["1", 1],
        "width": "{{width}}",
        "height": "{{height}}",
        "crop_w": 0,
        "crop_h": 0,
        "target_width": "{{width}}",
        "target_height": "{{height}}"
      }
    },
    "4": {
      "class_type": "EmptyLatentImage",
      "inputs": {
        "width": "{{width}}",
        "height": "{{height}}",
        "batch_size": 1
      }
    },
    "5": {
      "class_type": "KSampler",
      "inputs": {
        "model": ["1", 0],
        "positive": ["2", 0],
        "negative": ["3", 0],
        "latent_image": ["4", 0],
        "seed": "{{seed}}",
        "steps": 30,
        "cfg": 7.5,
        "sampler_name": "dpmpp_2m_sde",
        "scheduler": "karras",
        "denoise": 1.0
      }
    },
    "6": {
      "class_type": "CheckpointLoaderSimple",
      "inputs": {
        "ckpt_name": "sd_xl_refiner_1.0.safetensors"
      }
    },
    "7": {
      "class_type": "KSampler",
      "inputs": {
        "model": ["6", 0],
        "positive": ["2", 0],
        "negative": ["3", 0],
        "latent_image": ["5", 0],
        "seed": "{{seed}}",
        "steps": 10,
        "cfg": 7.5,
        "sampler_name": "dpmpp_2m_sde",
        "scheduler": "karras",
        "denoise": 0.3
      }
    },
    "8": {
      "class_type": "VAEDecode",
      "inputs": {
        "samples": ["7", 0],
        "vae": ["1", 2]
      }
    },
    "9": {
      "class_type": "SaveImage",
      "inputs": {
        "images": ["8", 0],
        "filename_prefix": "portrait_{{id}}"
      }
    }
  },
  "outputs": {
    "image": "9"
  },
  "parameters": {
    "id": {
      "type": "string",
      "required": true
    },
    "prompt": {
      "type": "string",
      "required": true
    },
    "style_base": {
      "type": "string",
      "default": "classical oil painting, museum quality, dignified portrait, warm lighting, detailed face"
    },
    "negative_prompt": {
      "type": "string",
      "default": "cartoon, anime, modern clothing, blurry, low quality, watermark, text, signature"
    },
    "width": {
      "type": "integer",
      "default": 512
    },
    "height": {
      "type": "integer",
      "default": 768
    },
    "seed": {
      "type": "integer",
      "default": 42
    }
  }
}
