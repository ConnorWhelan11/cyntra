{
  "compilerOptions": {
    "target": "ES2021",
    "useDefineForClassFields": true,
    "lib": ["ES2021", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@quantum-field": ["../../packages/ag-ui-ext/src/components/three/QuantumField/index.ts"],
      "@quantum-field/*": ["../../packages/ag-ui-ext/src/components/three/QuantumField/*"]
    }
  },
  "include": ["src"]
}

